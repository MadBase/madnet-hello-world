    "assert": "^2.0.0",
    "autoprefixer": "10.4.2",
    "better-docs": "2.7.1",
    "buffer": "^6.0.3",
    "postcss": "8.4.5",
    "redux-devtools-extension": "2.13.9",
    "stream": "0.0.2",
    "tailwindcss": "3.0.15"
  }
