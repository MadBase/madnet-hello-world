  "dependencies": {
    "@semantic-ui-react/css-patch": "1.0.0",
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.5.0",
    "ethers": "5.5.3",
    "jsdoc": "3.6.10",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.6",
    "react-router-dom": "6.2.1",
    "react-scripts": "5.0.0",
    "redux-thunk": "2.4.1",
    "semantic-ui-css": "2.4.1",
